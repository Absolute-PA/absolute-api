{
  "name": "nestjs-ecommerce",
  "version": "0.0.68",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build  && javascript-obfuscator ./dist --output ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "HTTPS=true nest start --exec 'node -r newrelic'",
    "start-https": "HTTPS=true  node -r newrelic ./dist/main.js",
    "dev-https": "HTTPS=true yarn dev --exec 'node -r newrelic'",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:db": "docker run --network absolutepa --name absolutepadb -d -p 27017:27017 mongo:4.4.8-focal",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compile": "tsc -b tsconfig.json --incremental",
    "docker:build": "docker build -t tien1106/absolutepa:nodeweb-master-api .",
    "deploy": "./scripts/prepareBuild.sh",
    "deployUat": "./scripts/prepareBuildUat.sh",
    "release": "./scripts/prepareBuildRelease.sh",
    "docker:save": "docker save -o ./out/nodeweb-master-api.tar nodeweb-master-api"
  },
  "dependencies": {
    "@agent-ly/nestjs-agenda": "^2.0.4",
    "@aws-sdk/client-cloudwatch-logs": "^3.414.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/platform-socket.io": "^9.4.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/websockets": "^9.4.0",
    "@types/multer": "^1.4.7",
    "agenda": "^5.0.0",
    "aws-sdk": "^2.1460.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "gtts": "^0.2.1",
    "loudness": "^0.4.1",
    "mongoose": "^6.8.0",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.9.3",
    "newrelic": "^11.1.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "peer": "^1.0.0",
    "play-sound": "^1.1.5",
    "puppeteer": "22.12.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "winston": "^3.10.0",
    "winston-cloudwatch": "^6.2.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "javascript-obfuscator": "^4.1.1",
    "jest": "28.1.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint \"{src,apps,libs,test}/**/*.ts\" --cache --fix --quiet",
      "prettier --write \"src/**/*.ts\""
    ]
  }
}
